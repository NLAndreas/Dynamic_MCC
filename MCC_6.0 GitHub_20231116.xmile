	<model name="Consumers">
		<variables>
			<stock name="On grid\nhouseholds" access="output" isee:autocreated="true">
				<eqn>20000</eqn>
				<inflow>building_on_grid</inflow>
				<non_negative/>
				<units>units</units>
			</stock>
			<flow name="building\non grid" access="output" isee:autocreated="true">
				<eqn>MIN(perceived_surplus_capacity/CONNECTION_DELAY, CONSUMER_GROWTH_RATE)
*GRID_GROWTH_SWITCH</eqn>
				<non_negative/>
				<units>units/year</units>
			</flow>
			<aux name="CONSUMER GROWTH\nRATE" access="input">
				<eqn>300</eqn>
				<units>units/year</units>
			</aux>
			<aux name="CONNECTION DELAY">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="surplus capacity">
				<eqn>&quot;added_capacity_(units)&quot;+
(freed_up_capacity + freed_up_capacity_1 + freed_up_capacity_2)*
INTERACTION_SWITCH -
On_grid_households

+INITIAL_CAPACITY


{GW.&quot;added_capacity_(units)&quot;-(On_grid_households[Permanent]+On_grid_households[Parttime]-INIT(On_grid_households[Permanent])-(On_grid_households[Parttime]))
+GWR.freed_up_capacity*Dyn.INTERACTION_SWITCH}</eqn>
				<units>units</units>
			</aux>
			<aux name="INTERACTION SWITCH" access="input">
				<eqn>0</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="freed up\ncapacity" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>units</units>
			</aux>
			<aux name="added capacity\n(units)" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>units</units>
			</aux>
			<aux name="GRID GROWTH SWITCH" access="input">
				<eqn>0</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="freed up\ncapacity 1" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>units</units>
			</aux>
			<aux name="freed up\ncapacity 2" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>units</units>
			</aux>
			<group name="Household growth" run="false">
				<entity name="GRID_GROWTH_SWITCH"/>
				<entity name="CONNECTION_DELAY"/>
				<entity name="freed_up_capacity_1"/>
				<entity name="perceived_surplus_capacity"/>
				<entity name="surplus_capacity"/>
				<entity name="On_grid_households"/>
				<entity name="building_on_grid"/>
				<entity name="INITIAL_CAPACITY"/>
				<entity name="total_water_demand"/>
				<entity name="BASELINE_WATER_USE_PER_UNIT"/>
				<entity name='"added_capacity_(units)"'/>
				<entity name="freed_up_capacity"/>
				<entity name="INTERACTION_SWITCH"/>
				<entity name="freed_up_capacity_2"/>
				<entity name="PERCEPTION_DELAY"/>
				<entity name="CONSUMER_GROWTH_RATE"/>
			</group>
			<aux name="INITIAL CAPACITY">
				<eqn>20000</eqn>
				<units>units</units>
			</aux>
			<aux name="total water demand" access="output" isee:autocreated="true">
				<eqn>On_grid_households*BASELINE_WATER_USE_PER_UNIT</eqn>
				<units>Cubic Meters/year</units>
