	<model name="Policy_Mix">
		<variables>
			<aux name="SEK/m3\nutilized" access="output" isee:autocreated="true">
				<eqn>IF water_utilized = 0
THEN 0
ELSE EAC/water_utilized</eqn>
				<units>SEK/meter^3</units>
			</aux>
			<aux name="EAC" access="output" isee:autocreated="true">
				<eqn>DISCOUNT_RATE_1*(Present_Value_Municipality+Present_Value_1+Present_Value_2+Present_Value)//
annuity_factor/ONE_YEAR</eqn>
				<units>SEK/year</units>
			</aux>
			<aux name="total installed\npotential" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>cubic meter/year</units>
			</aux>
			<aux name="total installed\npotential 1" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
