THEN PULSE( 
(((CAPEX[Capital_investment]+
CAPEX[Installation]+
CAPEX[Storage_tank]))/
discounting_factor_1), 2020, 100)

- PULSE(
(((&quot;value_remaining_at_end_of_simulation_-_HHS&quot;[Capital_investment]+
&quot;value_remaining_at_end_of_simulation_-_HHS&quot;[Installation]+
&quot;value_remaining_at_end_of_simulation_-_HHS&quot;[Storage_tank]))/discounting_factor_1), 2045, 100)


ELSE 0</eqn>
				<units>SEK/units/year</units>
			</flow>
			<flow name="HHS OPEX" access="output" isee:autocreated="true">
				<eqn>IF TIME &lt; INVESTMENT_YEAR_GWR
THEN 0
ELSE
OPEX_PER_UNIT/discounting_factor_1</eqn>
				<non_negative/>
				<units>SEK/units/year</units>
			</flow>
