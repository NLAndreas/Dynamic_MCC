	<model name="Water_Price">
		<variables>
			<stock name="Water Price" access="output" isee:autocreated="true">
				<eqn>{16.82*0

+1.36719*0}

(yearly_costs*(1-FEE_FRACTION))/yearly_GW_use</eqn>
				<inflow>Δ_price</inflow>
				<units>SEK/meter^3</units>
			</stock>
			<stock name="Yearly Fee" access="output" isee:autocreated="true">
				<eqn>{(746 + 613)*0

+825*0}

(yearly_costs*FEE_FRACTION)/On_grid_households

{Only Water = 746 + 613
Water + Grey Water = 1776 + 1460}</eqn>
				<inflow>Δ_fee</inflow>
				<units>SEK/units/year</units>
			</stock>
			<flow name="Δ price">
				<eqn>(desired_water_price-Water_Price)/WATER_PRICE_ADJUSTMENT_TIME</eqn>
				<units>SEK/meter^3/Years</units>
			</flow>
			<flow name="Δ fee">
				<eqn>(desired_yearly_fee-Yearly_Fee)/WATER_PRICE_ADJUSTMENT_TIME</eqn>
				<units>SEK/units/year/Years</units>
			</flow>
			<aux name="balance">
				<eqn>yearly_revenues-yearly_costs
{GW.yearly_costs}</eqn>
				<units>SEK/year</units>
			</aux>
			<aux name="DESIRED BALANCE">
				<eqn>0</eqn>
				<units>SEK/year</units>
			</aux>
			<aux name="yearly GW\nuse" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>meter^3/year</units>
			</aux>
			<stock name="On grid\nhouseholds" access="input">
				<eqn>20000</eqn>
				<non_negative/>
				<units>units</units>
			</stock>
			<aux name="yearly\nrevenues">
				<eqn>yearly_GW_use*Water_Price
+
On_grid_households*Yearly_Fee</eqn>
				<units>SEK/year</units>
			</aux>
			<aux name="yearly costs" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>SEK/year</units>
			</aux>
			<aux name="desired\nwater price">
				<eqn>Water_Price +
balance_gap*(1-FEE_FRACTION)/yearly_GW_use</eqn>
				<units>SEK/meter^3</units>
			</aux>
			<aux name="desired\nyearly fee">
				<eqn>Yearly_Fee+
(balance_gap*FEE_FRACTION)/On_grid_households</eqn>
				<units>SEK/units/year</units>
			</aux>
			<aux name="FEE FRACTION">
				<eqn>0.35</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="WATER PRICE\nADJUSTMENT TIME">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="balance gap">
				<eqn>DESIRED_BALANCE-balance</eqn>
				<units>SEK/year</units>
			</aux>
		</variables>
